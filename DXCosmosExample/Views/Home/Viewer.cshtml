@model DXCosmosExample.Models.ReportModel
@{
    ViewData["Title"] = "Web Document Viewer";
}
<h2>@ViewData["Title"].</h2>
<h3>@ViewData["Message"]</h3>

@*<link rel="stylesheet" type="text/css" href="~/node_modules/jquery-ui/themes/base/core.css" />
    <link rel="stylesheet" type="text/css" href="~/node_modules/jquery-ui/themes/base/resizable.css" />
    <link rel="stylesheet" type="text/css" href="~/node_modules/jquery-ui/themes/base/draggable.css" />*@

<link href="~/node_modules/jquery-ui-dist/jquery-ui.min.css" rel="stylesheet" />

<script src="~/node_modules/jquery/dist/jquery.min.js"></script>
<script src="~/node_modules/jquery-ui-dist/jquery-ui.min.js"></script>

<script src="~/node_modules/knockout/build/output/knockout-latest.js"></script>

<script src="~/node_modules/devextreme/dist/js/dx.all.js"></script>
<link href="~/node_modules/devextreme/dist/css/dx.common.css" rel="stylesheet" />
<link href="~/node_modules/devextreme/dist/css/dx.light.css" rel="stylesheet" />

<script src="~/node_modules/@@devexpress/analytics-core/dist/js/dx-analytics-core.js"></script>
<script src="~/node_modules/devexpress-reporting/dist/js/dx-webdocumentviewer.js"></script>
<link href="~/node_modules/@@devexpress/analytics-core/dist/css/dx-analytics.common.css" rel="stylesheet" />
<link href="~/node_modules/@@devexpress/analytics-core/dist/css/dx-analytics.light.css" rel="stylesheet" />
<link href="~/node_modules/devexpress-reporting/dist/css/dx-webdocumentviewer.css" rel="stylesheet" />

@{
    var viewer = Html.DevExpress().WebDocumentViewer("viewer")
        .Bind(Model.ReportUrl)
        .Height("700px")
        .ClientSideModelSettings(x => {
            x.IncludeCldrData = false;
            x.IncludeCldrSupplemental = false;
            x.IncludeLocalization = false;
        });
}
@viewer